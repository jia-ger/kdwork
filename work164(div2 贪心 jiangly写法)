https://codeforces.com/contest/2146/problem/B

#include <bits/stdc++.h>
using namespace std;
#define itn int
#define icn cin
#define int long long
#define endl '\n'
typedef pair<int,int> PII;
const int N=1e5+10;
const int INF=1e18;
const int mod=998244353;
#define ll __int128


void solve()
{    
    int n, m;
    cin >> n >> m;
    
    vector<int> cnt(m);
    vector<std::vector<int>> s(n);
    for (int i = 0; i < n; i++) {
        int l;
        std::cin >> l;
        s[i].resize(l);
        for (int j = 0; j < l; j++) {
            cin >> s[i][j];
            s[i][j]--;
            cnt[s[i][j]]++;
        }
    }
    if (count(cnt.begin(), cnt.end(), 0)) {
        cout << "NO\n";
        return;
    }
    
    int ans = 1;//全选是1种方案 

    for (int i = 0; i < n; i++) {
        bool ok = true;
        for (auto x : s[i]) {
            if (cnt[x] == 1) {
                ok = false;
            }
        }
        ans += ok;//有两项 额外的就ok了 
    }
    if (ans >= 3) {
        std::cout << "YES\n";
    } else {
        std::cout << "NO\n";
    }
}



signed main(){
   cin.tie(0)->sync_with_stdio(0);	 
   int T=1; 
   cin>>T;
   while(T--)solve();
   
    return 0;
}
